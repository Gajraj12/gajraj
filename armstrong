#include <stdio.h>
#include <math.h>
int main()
{
    int a, c;
    double s = 0;
    printf("enter a number:");
    scanf("%d", &a);
    double b = a, e;
    c = a;
    int d = a;
    while (a > 0)
    {
        s = s + 1;
        a /= 10;
    }
    while (c > 0)
    {
        b = c % 10;
        e = e + pow(b, s);
        c /= 10;
    }
    if (d==e)
    {
        printf("Armstrong Number");
    }
    else
    {
        printf("Not a Armstrong Number");
    }
    return 0;
}
